<style>
    .content {

  display: flex;
  align-items: center;
  min-height: 100vh;
  flex-direction: column;
}


.wrapper {
  width: 90%;
  height: 100vh;
  position: relative;
  overflow: hidden;
}

.wrapper::after {
  width: 100%;
  height: 100vh;
  content: "";
  position: absolute;
}

.box {

  width: auto;
  height: 100vh;
  position: absolute;
  font-size: 25px;
  line-height: 50px;
  text-align: center;
  color: #105ee4;
}

.boxes {
  position: relative;
    left: -50px;
}

.logo-svg {
  width: 100px;
  height: 100px;
  color: #9ca3af;
}

.logo-svg * {
  fill: currentColor !important;
  stroke: currentColor !important;
}

</style>

<script>

import gsap from "gsap";

//initially colorize each box and position in a row
gsap.set(".box", {
  x: (i) => i * 200
});


gsap.to(".box", {
  duration: 10,
  ease: "power0",
  x: "+=2000", //move each box 500px to right
  modifiers: {
    x: gsap.utils.unitize(x => parseFloat(x) % 2000) //force x value to be between 0 and 500 using modulus
  },
  repeat: -1
});

// Animaci칩n hover de rotaci칩n para los 칤conos de redes
const socialSvgs = gsap.utils.toArray("#logosredes svg");
socialSvgs.forEach((icon) => {
  icon.style.transformOrigin = "50% 50%";
  const parent = icon.closest("a") || icon;

  parent.addEventListener("mouseenter", () => {
    gsap.to(icon, { rotation: "+=360", duration: 0.6, ease: "power2.out" });
  });

  parent.addEventListener("mouseleave", () => {
    gsap.to(icon, { rotation: 0, duration: 0.5, ease: "power2.out" });
  });
});


const tl = gsap.timeline({ defaults: { ease: "power3.out", duration: 0.8 } });
tl.from(".hero-card", { y: 30, opacity: 0 })
  .from(".hero-title", { y: 30, opacity: 0 }, "-=0.4")
  .from(".hero-pill", { y: 25, opacity: 0 }, "-=0.45")
  .from(".hero-country", { y: 25, opacity: 0 }, "-=0.45")
  .from(".hero-copy p", { y: 20, opacity: 0 }, "-=0.4")
  .from("#logosredes a", { y: 15, opacity: 0, stagger: 0.08 }, "-=0.45")
  .from(".box", { autoAlpha: 0, y: 20, stagger: 0.05 }, "-=0.6");

gsap.from("#boton ",{y:20,opacity:0,duration:0.8,ease:"power3.out",delay:2});

</script>
